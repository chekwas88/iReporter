  },
  "homepage": "https://github.com/chekwas88/iReporter#readme",
  "devDependencies": {
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.9.0",
